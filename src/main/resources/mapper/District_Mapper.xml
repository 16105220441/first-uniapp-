<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.example.shoppingspring.mapper.District_Mapper">
    <select id="selectByCityId"
            resultType="tlDistrict">
        select  DistrictName from shopping_datasource.tl_district where
            CityID = #{cityId}
    </select>
</mapper>
